      <mdui-icon id="bot__task-dialog-step2-icon" name="grid_on"></mdui-icon>
      <div style="margin-left: 0.25rem;">
        <div id="bot__task-dialog-step2-id" style="font-weight: bold;">ae.getCpus</div>
